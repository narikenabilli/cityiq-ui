{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "comma-dangle": ["error", {
      "objects": "only-multiline",
      "arrays": "only-multiline",
      "functions": "never"
    }]
  }
}
