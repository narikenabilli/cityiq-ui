{
  "name": "predix-cityiq-google-map",
  "version": "1.0.0",
  "description": "Simple proof-of-concept application, that combines together services provided by CityIQ (Pedestrian and Traffic data), Predix (Assets and Cloud), and Google (Maps and Places)",
  "main": "bin/www",
  "engines": {
    "node": ">=7.5.0",
    "npm": ">=4.2.1"
  },
  "scripts": {
    "start": "node ./bin/www",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --env=production",
    "build:dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --env=development",
    "dev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "postinstall": "npm run build",
    "data:view": "node ./src/server/tools/view-advices.js",
    "data:clear": "node ./src/server/tools/clear-advices.js",
    "lint": "eslint --ext .js ./src",
    "lint:fix": "eslint --fix --ext .js ./src"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0-beta.5",
    "babel-plugin-react-css-modules": "^3.3.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "express": "^4.15.4",
    "express-logging": "^1.1.1",
    "extract-css-chunks-webpack-plugin": "^2.0.17",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "predix-uaa-client": "^1.2.2",
    "prop-types": "^15.5.10",
    "querystring": "^0.2.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-google-maps": "^8.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-redux-toastr": "^7.1.5",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "request-promise-native": "^1.0.4",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uuid": "^3.1.0",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0",
    "whatwg-fetch": "^2.0.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "nodemon": "^1.11.0"
  },
  "keywords": [
    "Predix",
    "CityIQ",
    "Google Maps"
  ],
  "author": "",
  "license": "ISC"
}
